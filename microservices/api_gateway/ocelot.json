{
  "Routes": [
    {
      "DownstreamPathTemplate": "/search",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "frontend",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/search",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      "DownstreamPathTemplate": "/collect",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "twitter_collector",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/collect",
      "UpstreamHttpMethod": [ "Post" ]
    },
    {
      "DownstreamPathTemplate": "/analyze",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "sentiment_analyzer",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/analyze",
      "UpstreamHttpMethod": [ "Post" ]
    },
    {
      "DownstreamPathTemplate": "/store",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "datastorage",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/store",
      "UpstreamHttpMethod": [ "Post" ]
    },
    {
      "DownstreamPathTemplate": "/visualize",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "visualization",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/visualize",
      "UpstreamHttpMethod": [ "Get" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}
